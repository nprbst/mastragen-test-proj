{
  "name": "mastragen-test-proj",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.0.10",
    "mastra": "^1.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.13.0"
  },
  "scripts": {
    "dev": "mastra dev",
    "dev:ui": "bun run --filter ./packages/ui dev",
    "build": "mastra build",
    "build:ui": "bun run --filter ./packages/ui build",
    "start": "mastra start",
    "phoenix:up": "docker start arize-phoenix 2>/dev/null || docker run --pull=always -d --name arize-phoenix -p 6006:6006 -e PHOENIX_SQL_DATABASE_URL=sqlite:///:memory: arizephoenix/phoenix:latest",
    "phoenix:down": "docker stop arize-phoenix"
  },
  "dependencies": {
    "@mastra/arize": "^1.0.0",
    "@mastra/core": "^1.0.4",
    "@mastra/libsql": "^1.0.0",
    "@mastra/loggers": "^1.0.0",
    "@mastra/memory": "^1.0.0",
    "@mastra/observability": "^1.0.0",
    "zod": "^4"
  }
}
